<br>
<div class="well">

  <h2>Welcome to <%= current_user.role%>'s Dashboard</h2>

  <% if  current_user.role == "admin" %>
    <div class="pull-right">
      <div class="btn-group" role="group" aria-label="Default button group">
        <button type="button" class="btn btn-default">
          <%= link_to "Show Users", doctors_path %>
        </button>
        <button type="button" class="btn btn-default">
          <%= link_to "Show Appointments", appointments_path %>
        </button>
      </div>
    </div>
    <br>
    <hr>
    <img data-src="holder.js/140x140" class="img-thumbnail" alt="140x140" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 140px; height: 140px;">
    <img data-src="holder.js/140x140" class="img-thumbnail" alt="140x140" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 140px; height: 140px;">
    <img data-src="holder.js/140x140" class="img-thumbnail" alt="140x140" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 140px; height: 140px;">
  <% end %>

  <% if  current_user.role == "doctor" %>
    <div class="pull-right">
      <div class="btn-group" role="group" aria-label="Default button group">
        <button type="button" class="btn btn-default">
          <%= link_to "Show Appointments", appointments_path %>
        </button>
      </div>
    </div>
    <br>
    <hr>
    <img data-src="holder.js/140x140" class="img-thumbnail" alt="140x140" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 140px; height: 140px;">
    <img data-src="holder.js/140x140" class="img-thumbnail" alt="140x140" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 140px; height: 140px;">
    <img data-src="holder.js/140x140" class="img-thumbnail" alt="140x140" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 140px; height: 140px;">
  <% end %>

  <% if  current_user.role == "customer" %>
    <br>
    <hr>
    <%= render "appointments/customer_appointments" %>
  <% end %>

  <% if  current_user.role == "receptionist" %>
    <br>
    <hr>
    <div class="pull-right">
      <%= link_to "Show Appointments", "/appointments" %>
    </div>
    <br>

  <% end %>


</div>
